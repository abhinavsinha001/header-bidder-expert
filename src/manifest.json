{
    "manifest_version":         2,
    "name":                     "Header Bidder Expert",
    "version":                  "1.0.4",
    "description":              "This extension provides insights into the the header bidding activity happening on the websites.",
    "author":                   "Prebid.org",
    "homepage_url":             "http://prebid.org",
    // HB_IF_CHROME
    "minimum_chrome_version":   "41",
    // END_HB_IF_CHROME
    // HB_IF_FF
    "applications": {
        "gecko": {
            "id": "header-bidder-expert@prebid.org",
            "strict_min_version": "56.0"
        }
    },
    // END_HB_IF_FF
    "icons": {
        "16":   "img/icons/icon-16px.png",
        "32":   "img/icons/icon-32px.png",
        "48":   "img/icons/icon-48px.png",
        "64":   "img/icons/icon-64px.png",
        "128":  "img/icons/icon-128px.png"
    },
    "background": {
        "scripts": [
            "js/worlds/background/entry-bundle.js"
        ]
    },
    "page_action": {
        "default_title": "Header Bidder Expert",
        "default_icon": {
            "16":   "img/icons/icon-16px.png",
            "32":   "img/icons/icon-32px.png",
            "48":   "img/icons/icon-48px.png",
            "64":   "img/icons/icon-64px.png",
            "128":  "img/icons/icon-128px.png"
        }
    },
    "permissions": [
        "activeTab",
        "webRequest",
        "webNavigation",
        "<all_urls>"
    ]
}
